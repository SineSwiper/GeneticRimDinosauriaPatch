<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="WoolBase">
		<defName>GR_DinoFeatherWool</defName>
		<label>dino featherwool</label>
		<description>This seems to be an amalgamation of feathers and wool, perfectly interweaved. Gives excellent defense against blunt attacks, and is quite warm.</description>
		<graphicData>
			<texPath>Things/Item/GR_DinoFeatherWool</texPath>
			<graphicClass>Graphic_Single</graphicClass>		
		</graphicData>
		<tradeability>Sellable</tradeability>
		<stuffProps>
			<color>(131,78,47)</color>
			<statOffsets>
				<ArmorRating_Heat>0.15</ArmorRating_Heat>
				<ArmorRating_Electric>0.15</ArmorRating_Electric>
			</statOffsets>
			<statFactors>
				<ArmorRating_Blunt>1.9</ArmorRating_Blunt>
				<ArmorRating_Heat>2</ArmorRating_Heat>
				<Insulation_Cold>3</Insulation_Cold>
				<Insulation_Heat>3</Insulation_Heat>
			</statFactors>
		</stuffProps>
	</ThingDef>
	
	<ThingDef ParentName="WoolBase">
		<defName>GR_ScalyWool</defName>
		<label>dino scaly wool</label>
		<description>This seems to be an amalgamation of keratinous scales and wool, perfectly interweaved. Gives excellent defense against sharp attacks, and is quite warm.</description>
		<graphicData>
			<texPath>Things/Item/GR_ScalyWool</texPath>
			<graphicClass>Graphic_Single</graphicClass>		
		</graphicData>
		<tradeability>Sellable</tradeability>
		<stuffProps>
			<color>(152,162,169)</color>
			<statOffsets>
				<ArmorRating_Heat>0.15</ArmorRating_Heat>
				<ArmorRating_Electric>0.15</ArmorRating_Electric>
			</statOffsets>
			<statFactors>
				<ArmorRating_Sharp>1.9</ArmorRating_Sharp>
				<ArmorRating_Heat>2</ArmorRating_Heat>
				<Insulation_Cold>3</Insulation_Cold>
				<Insulation_Heat>3</Insulation_Heat>
			</statFactors>
		</stuffProps>
	</ThingDef>
	
	
	        
	
 
  

</Defs>
